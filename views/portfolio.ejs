<style>
.skills-achievements { margin-top: 40px; }
.skills-achievements .title-wrapper { display: flex; align-items: center; gap: 15px; margin-bottom: 25px; }
.skills-achievements .icon-box {
  background: rgba(154, 121, 1, 0.15);
  color: #c08000;
  font-size: 20px;
  padding: 8px;
  border-radius: 10px;
}
.skills-achievements .h3 { color: #fff; }

/* Skill Categories */
.skill-category { margin-bottom: 30px; }
.skill-subtitle { font-size: 1.2rem; font-weight: 600; color: #fff; margin-bottom: 12px; }

/* Carousel */
.skills-carousel {
  overflow: hidden;
  width: 100%;
  position: relative;
}

.skills-track {
  display: flex;
  gap: 12px;
  animation: scroll 20s linear infinite;
}

.skill-slide {
  flex: 0 0 auto;
  background: rgba(255, 255, 255, 0.05);
  border: 1px solid rgba(255,255,255,0.08);
  border-radius: 10px;
  padding: 8px 10px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  min-width: 70px;
  transition: transform 0.25s ease, box-shadow 0.25s ease;
}

.skill-slide img {
  width: 30px;
  height: 30px;
  margin-bottom: 4px;
}

.skill-slide span {
  color: #ffd166;
  font-size: 0.75rem;
  font-weight: 500;
  text-align: center;
}

.skill-slide:hover {
  box-shadow: 0 6px 16px rgba(209, 155, 9, 0.3);
  border-color: rgba(143,129,0,0.5);
}

/* Infinite scroll with duplicated slides for continuous loop */
@keyframes scroll {
  0% { transform: translateX(0); }
  100% { transform: translateX(-50%); }
}

/* Responsive */
@media(max-width:768px){
  .skills-track { animation-duration: 30s; gap: 10px; }
  .skill-slide { min-width: 60px; padding: 6px; }
  .skill-slide img { width: 25px; height: 25px; }
  .skill-slide span { font-size: 0.7rem; }
}
</style>

<script>
const track = document.querySelector('.skills-track');
const slides = Array.from(track.children);

// Clone all slides
slides.forEach(slide => {
  const clone = slide.cloneNode(true);
  track.appendChild(clone);
});
</script>

